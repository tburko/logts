{
	"compilerOptions": {
		"declaration": true,
		"experimentalDecorators": true,
		"module": "commonjs",
		"noImplicitAny": true,
		"outDir": "build",
		"preserveConstEnums": true,
		"removeComments": false,
		"sourceMap": true,
		"suppressImplicitAnyIndexErrors": true,
		"target": "ES5"
	},
	"exclude": [
		"node_modules",
		"build"
	]
}
